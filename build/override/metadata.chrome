[default]
inherit = metadata.common

[general]
basename = adblockpluschrome
permissions = tabs
  activeTab
  http://*/*
  https://*/*
  contextMenus
  webRequest
  webRequestBlocking
  webNavigation
  storage
  unlimitedStorage
  notifications
  contentSettings
  cookies
  management
  tabs
  https://platform.twitter.com/js/*
options = /operando/operando.html
browserAction = operando/assets/images/icons/operando/abp-19.png operando/assets/images/icons/operando/abp-38.png popup.html
icons = operando/assets/images/icons/operando/abp-16.png operando/assets/images/icons/operando/abp-32.png operando/assets/images/icons/operando/detailed/abp-48.png
  operando/assets/images/icons/operando/detailed/abp-64.png operando/assets/images/icons/operando/detailed/abp-128.png
managedStorageSchema = managed-storage-schema.json

[compat]
chrome = 29.0

[mapping]
icons/abp-19.png = operando/assets/images/icons/abp-19.png
icons/abp-19-whitelisted.png = operando/assets/images/icons/abp-19-whitelisted.png
icons/abp-19-notification-critical.png = operando/assets/images/icons/abp-19-notification-critical.png
icons/abp-19-notification-information.png = operando/assets/images/icons/abp-19-notification-information.png
icons/abp-38.png = operando/assets/images/icons/abp-38.png
icons/abp-38-whitelisted.png = operando/assets/images/icons/abp-38-whitelisted.png
icons/abp-38-notification-critical.png = operando/assets/images/icons/abp-38-notification-critical.png
icons/abp-38-notification-information.png = operando/assets/images/icons/abp-38-notification-information.png
icons/detailed/abp-48.png = operando/assets/images/icons/detailed/abp-48.png
ext/devtools.js = chrome/ext/devtools.js
devtools.html = chrome/devtools.html
devtools.js = chrome/devtools.js
devtools-panel.html = adblockplusui/devtools-panel.html
devtools-panel.js = adblockplusui/devtools-panel.js
skin/devtools-panel.css = adblockplusui/skin/devtools-panel.css
managed-storage-schema.json = chrome/managed-storage-schema.json